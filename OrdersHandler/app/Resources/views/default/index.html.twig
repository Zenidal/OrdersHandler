{% extends 'base.html.twig' %}

{% block body %}
    <div class="home">
        <div class="container-fluid">
            <div class="home">
                <div class="home-error">
                    {% if errorMessages is defined %}
                        {% for error in errorMessages %}
                            <div class="error text-danger">
                                {{ error }}
                            </div>
                        {% endfor %}
                    {% endif %}
                </div>
                <legend><h1>Homepage</h1></legend>
            </div>
        </div>
    </div>
{% endblock %}
